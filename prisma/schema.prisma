generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Livro {
  id        Int    @id @default(autoincrement())
  nome      String @db.VarChar(33)
  imagem    String @db.VarChar(255)
  selecionado Int
}

model Filme {
  id        Int    @id @default(autoincrement())
  nome      String @db.VarChar(33)
  imagem    String @db.VarChar(255)
}
